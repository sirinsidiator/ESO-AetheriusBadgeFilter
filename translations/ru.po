#. TRANSLATORS: Title of the filter window in the guild roster menu
#: src/FilterWindow.lua:47
msgid "Badge Filter"
msgstr "Фильтр Значков"

#. TRANSLATORS: Entry in the options menu of the filter window for rescanning the roster for badges
#: src/FilterWindow.lua:71
msgid "Refresh Badges"
msgstr "Обновить Значки"

#. TRANSLATORS: Entry in the options menu of the filter window for showing badges from the data file for the current guild
#: src/FilterWindow.lua:75
msgid "Show Defined Badges"
msgstr "Показать Созданные Значки"

#. TRANSLATORS: Entry in the options menu of the filter window for showing badges found in the roster for the current guild
#: src/FilterWindow.lua:81
msgid "Show Scanned Badges"
msgstr "Показать Найденные Значки"

#. TRANSLATORS: Entry in the options menu of the filter window for hiding the member count on each badge
#: src/FilterWindow.lua:89
msgid "Hide Member Count"
msgstr "Скрыть Количество Награжденных"

#. TRANSLATORS: Entry in the options menu of the filter window for showing the member count on each badge
#: src/FilterWindow.lua:95
msgid "Showing Member Count"
msgstr "Показать Количество Награжденных"

#. TRANSLATORS: Entry in the options menu of the filter window to unlock the position of the window so it can be moved around
#: src/FilterWindow.lua:103
msgid "Unlock Window"
msgstr "Разблокировать"

#. TRANSLATORS: Entry in the options menu of the filter window to lock the position of the window
#: src/FilterWindow.lua:106
msgid "Lock Window"
msgstr "Заблокировать"

#. TRANSLATORS: Entry in the options menu of the filter window to reset the position of the window
#: src/FilterWindow.lua:108
msgid "Reset Window"
msgstr "Восстановить"

#. TRANSLATORS: Entry in the options menu of the filter window to hide the window
#: src/FilterWindow.lua:112
msgid "Hide Window"
msgstr "Скрыть"

#. TRANSLATORS: Tooltip for the button in the guild roster menu to show or hide the filter window
#: src/FilterWindow.lua:133
msgid "Toggle Aetherius Badge Filter Window"
msgstr "Изменить Режим Aetherius Badge Filter"

#. TRANSLATORS: Text shown in the filter window when there are no badges for the current guild
#: src/FilterWindow.lua:251
msgid "No Badges found"
msgstr "Значков Не Найдено"

#. TRANSLATORS: Entry for incrementing a badge value in the badge editor context menu
#: src/MemberNoteEditor.lua:86
msgid "Increment"
msgstr "Добавить"

#. TRANSLATORS: Entry for decrementing a badge value in the badge editor context menu
#: src/MemberNoteEditor.lua:90
msgid "Decrement"
msgstr "Убавить"

#. TRANSLATORS: Entry for updating a badge color in the badge editor context menu
#: src/MemberNoteEditor.lua:96
msgid "Update Color"
msgstr "Выбрать Цвет"

#. TRANSLATORS: Entry for updating a badge color in the badge editor context menu
#: src/MemberNoteEditor.lua:101
msgid "Remove"
msgstr "Удалить"
